#include<stdio.h>
void insertionSort(int arr[], int n){
    for(int i =1 ; i<n ; i++){
        int curr = arr[i];
        int j = i-1;
        while(j>=0 && arr[j]> curr ){
            arr[j+1] = arr[j];
            j--;     
           }
           arr[j+1]= curr;
    }
}
void printarr(int arr[], int n ){
    for(int i=0; i<n; i++){
        printf("%d  ",arr[i]);
    }
}
int main(){
    int arr[] = {5,2,1,3,4};
    int n = sizeof(arr)/sizeof(arr[0]);
    insertionSort(arr , n);
    printarr(arr, n);

}
